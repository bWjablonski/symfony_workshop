{% extends 'base.html.twig' %}

{% block title %}Insurance{% endblock %}

{% block body %}
    <h1>Insurance</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ insurance.id }}</td>
            </tr>
            <tr>
                <th>EndDate</th>
                <td>{{ insurance.endDate ? insurance.endDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ insurance.type.name }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ insurance.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ insurance.description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_insurance_index') }}">back to list</a>

    <a href="{{ path('app_insurance_edit', {'id': insurance.id}) }}">edit</a>

    {{ include('insurance/_delete_form.html.twig') }}
{% endblock %}
